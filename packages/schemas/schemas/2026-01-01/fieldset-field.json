          "description": "Postal or ZIP code",
          "type": "string",
          "minLength": 3,
          "maxLength": 20,
          "pattern": "^[A-Z0-9\\s-]+$"
        },
        "country": {
          "description": "ISO 3166-1 country code (e.g., \"US\", \"GB\", \"FRA\") or full country name",
          "type": "string",
          "minLength": 2,
          "maxLength": 100
        }
      },
      "required": [
        "line1",
        "locality",
        "region",
        "postalCode",
        "country"
      ],
      "additionalProperties": false,
      "id": "Address"
    },
    "Phone": {
      "type": "object",
      "properties": {
        "number": {
