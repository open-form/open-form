          "description": "Expression string - either inline expression or reference to a logic key",
          "type": "string",
          "minLength": 1,
          "maxLength": 2000
        }
      ]
    },
    "Coordinate": {
      "type": "object",
      "properties": {
        "lat": {
          "description": "Latitude in decimal degrees (WGS84), range -90 (South Pole) to 90 (North Pole)",
          "type": "number",
          "minimum": -90,
          "maximum": 90
        },
        "lon": {
          "description": "Longitude in decimal degrees (WGS84), range -180 (west) to 180 (east)",
          "type": "number",
          "minimum": -180,
          "maximum": 180
        }
      },
      "required": [
        "lat",
        "lon"
      ],
      "additionalProperties": false,
      "id": "Coordinate"
    },
    "Bbox": {
      "type": "object",
      "properties": {
        "southWest": {
          "description": "Southwest corner coordinate (minimum latitude and longitude)",
          "$ref": "https://schema.open-form.dev/2026-01-01.json/$defs/Coordinate"
        },
        "northEast": {
          "description": "Northeast corner coordinate (maximum latitude and longitude)",
          "$ref": "https://schema.open-form.dev/2026-01-01.json/$defs/Coordinate"
        }
      },
      "required": [
        "southWest",
        "northEast"
      ],
      "additionalProperties": false,
      "id": "Bbox"
    },
    "Money": {
      "type": "object",
      "properties": {
        "amount": {
          "description": "Monetary amount in the smallest currency unit (e.g., cents for USD) or as a decimal (e.g., 99.99 for USD). Negative values allowed for debts/credits",
          "type": "number"
        },
        "currency": {
          "description": "ISO 4217 alpha-3 currency code (e.g., USD, EUR, GBP)",
          "type": "string",
          "minLength": 3,
          "maxLength": 3,
          "pattern": "^[A-Z]{3}$"
        }
      },
      "required": [
        "amount",
        "currency"
      ],
      "additionalProperties": false,
      "id": "Money"
    },
    "Address": {
      "type": "object",
      "properties": {
        "line1": {
          "description": "Primary address line",
          "type": "string",
          "minLength": 1,
          "maxLength": 200
        },
        "line2": {
          "description": "Secondary address line",
          "type": "string",
          "maxLength": 200
        },
        "locality": {
          "description": "City or locality name",
          "type": "string",
          "minLength": 1,
          "maxLength": 200
        },
        "region": {
          "description": "State, province, or region name",
          "type": "string",
          "minLength": 1,
          "maxLength": 100
