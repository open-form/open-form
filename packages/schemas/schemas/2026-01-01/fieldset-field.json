        "legalName": {
          "description": "Legal or registered name of the organization",
          "type": "string",
          "minLength": 1,
          "maxLength": 200
        },
        "domicile": {
          "description": "Domicile",
          "type": "string",
          "minLength": 1,
          "maxLength": 100
        },
        "entityType": {
